{
    "mcpServers": {


      "postgres": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-postgres",
          "${SUPABASE_URI}"
        ],
        "transport": "stdio"
      },


      "marketing": {
        "command": "python",
        "args": [
            "server.py"
        ],
        "transport": "stdio"
      },

      
      "slack": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-slack"
        ],
        "env": {
          "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}",
          "SLACK_TEAM_ID": "${SLACK_TEAM_ID}",
          "SLACK_CHANNEL_IDS": "C091TJXMC8P"
        },
        "transport": "stdio"
      },

      "pd": {
      "command": "npx",
      "args": [
        "-y",
        "supergateway",
        "--sse",
        "https://mcp.pipedream.net/d05882ca-f5f1-414c-8cf9-b398e1455c73/gmail"
      ],
      "transport": "sse",
      "url": "https://mcp.pipedream.net/d05882ca-f5f1-414c-8cf9-b398e1455c73/gmail"
    }
    }
  }
